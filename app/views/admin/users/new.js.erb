$('#user').html("<%= j render(:partial => 'new') %>");
$(document).on("change", "#state", function(){
        var val = $(this).val();
        if (val == 223 || val == 224) {
            $('#othstate').val('').hide();
            $.ajax({
               url: "admin/users",
               method: "GET", 
               dataType: "json",
               data: { state : val },
               success: function(data) {
                   $('#state').html( data );
               }
            });
        }
        else {
           $('#state').val('').hide();
           $('#othstate').show();
        }
});